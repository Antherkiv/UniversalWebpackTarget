    "tslint-config-airbnb": "^5.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "file:node_packages/webpack",
    "webpack-cli": "^2.1.3",
    "webpack-core": "^0.6.9",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "file:node_packages/webpack-hot-middleware",
