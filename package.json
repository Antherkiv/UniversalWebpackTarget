{
  "name": "universal-webpack",
  "version": "0.1.0",
  "description": "Universal Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack --mode development --progress --verbose --config webpack.server.js && NODE_PATH=. node ./server.js",
    "build": "webpack --mode production --progress",
    "build:dev": "webpack --mode development --progress --verbose",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "postcss": {},
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)",
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "dependencies": {
    "color": "^3.0.0",
    "express": "^4.16.3",
    "formik": "^0.11.11",
    "gaikan": "file:node_packages/gaikan",
    "helmet": "^3.12.1",
    "hoist-non-react-statics": "^2.5.0",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.2.1",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "yup": "^0.25.1"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.37",
    "@types/jest": "^22.2.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-intl": "^2.3.8",
    "@types/react-motion": "0.0.26",
    "@types/react-redux": "^6.0.0",
    "@types/react-router": "^4.0.25",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.14",
    "@types/react-select": "^1.2.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/reactstrap": "^5.0.26",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack": "^4.4.0",
    "@types/webpack-dev-middleware": "^2.0.1",
    "@types/webpack-hot-middleware": "^2.16.3",
    "@types/yup": "^0.24.4",
    "assets-webpack-plugin": "file:node_packages/assets-webpack-plugin",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "glob": "^7.1.2",
    "jest": "^22.4.4",
