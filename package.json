{
  "name": "universal-webpack",
  "version": "0.1.0",
  "description": "Universal Webpack",
  "main": "index.js",
  "scripts": {
    "server": "NODE_PATH=. node ./libs/server/server.js",
    "start": "npm run build && npm run server",
    "start:dev": "npm run build:dev && npm run server",
    "start:dev-server": "webpack-dev-server --mode development --verbose",
    "build": "webpack --mode production --progress",
    "build:dev": "webpack --mode development --progress --verbose",
    "debug:server": "NODE_PATH=. node --inspect ./libs/server/server.js",
    "debug:start": "npm run build && npm run debug:server",
    "debug:start:dev": "npm run build:dev && npm run debug:server",
    "debug:build": "node --inspect $(npm bin)/webpack --mode production --progress",
    "debug:build:dev": "node --inspect $(npm bin)/webpack --mode development --progress --verbose",
    "debug": "npm run debug:start:dev",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "postcss": {},
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)",
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "dependencies": {
    "color": "^3.0.0",
    "express": "^4.16.3",
    "hogan-xpress": "^1.0.8",
    "hoist-non-react-statics": "^2.5.0",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/jasmine": "^2.8.7",
    "@types/jest": "^22.2.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack": "^4.4.0",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "glob": "^7.1.2",
    "jest": "^22.4.4",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
